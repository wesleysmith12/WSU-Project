<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:weightSum="1">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:weightSum="1">

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="File Name: "
        android:id="@+id/textView16"
        android:layout_gravity="right"
        android:textColor="#000000"
        android:textStyle="bold"/>

    <EditText
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:id="@+id/filename"
        android:layout_gravity="right" />

        <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Export"
        android:id="@+id/export"
        android:onClick="exportData" />

        <fragment
            android:layout_width="wrap_content"
            android:layout_height="227dp"
            android:name="com.example.wesle.wsuuioption1.OverallSumFragment"
            android:id="@+id/fragment18"
            tools:layout="@layout/fragment_overall_sum"
            android:layout_marginLeft="40dp" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Restart"
            android:id="@+id/restart"
            android:focusable="true"
            android:layout_weight="2.60" />

    </LinearLayout>

    <GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="994dp"
    android:layout_height="440dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.wesle.wsuuioption1.SummaryActivity"
    android:layout_gravity="center_horizontal"
        android:layout_weight="0.75">

    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="com.example.wesle.wsuuioption1.MovieSumFragment"
        android:id="@+id/fragment10"
        android:layout_row="0"
        android:layout_column="0"
        tools:layout="@layout/fragment_movie_sum" />

    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="com.example.wesle.wsuuioption1.TeaSumFragment"
        android:id="@+id/fragment11"
        android:layout_row="0"
        android:layout_column="1"
        tools:layout="@layout/fragment_tea_sum" />

    <fragment
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:name="com.example.wesle.wsuuioption1.SnackSumFragment"
        android:id="@+id/fragment12"
        android:layout_row="0"
        android:layout_column="2"
        tools:layout="@layout/fragment_snack_sum" />

    <fragment
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:name="com.example.wesle.wsuuioption1.ChangeSumFragment"
        android:id="@+id/fragment13"
        android:layout_row="0"
        android:layout_column="3"
        tools:layout="@layout/fragment_change_sum" />

    <fragment
        android:layout_width="221dp"
        android:layout_height="276dp"
        android:name="com.example.wesle.wsuuioption1.PhoneSumFragment"
        android:id="@+id/fragment14"
        android:layout_row="1"
        android:layout_column="0"
        tools:layout="@layout/fragment_phone_sum" />

    <fragment
        android:layout_width="236dp"
        android:layout_height="303dp"
        android:name="com.example.wesle.wsuuioption1.RecipeSumFragment"
        android:id="@+id/fragment15"
        android:layout_row="1"
        android:layout_column="1"
        tools:layout="@layout/fragment_recipe_sum" />

    <fragment
        android:layout_width="246dp"
        android:layout_height="293dp"
        android:name="com.example.wesle.wsuuioption1.TravelSumFragment"
        android:id="@+id/fragment16"
        android:layout_row="1"
        android:layout_column="2"
        tools:layout="@layout/fragment_travel_sum" />

    <fragment
        android:layout_width="324dp"
        android:layout_height="291dp"
        android:name="com.example.wesle.wsuuioption1.ExitSumFragment"
        android:id="@+id/fragment17"
        android:layout_row="1"
        android:layout_column="3"
        tools:layout="@layout/fragment_exit_sum" />

</GridLayout>

</LinearLayout>
